<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="efemerides_view_form" model="ir.ui.view">
        <field name="name">efemerides.view.form</field>
        <field name="model">efemerides.efemerides</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name" 
                        readonly="1"
                        />
                        <field name="contenido" />
                         <field name="imagen" filename="store_fname" />
                        <field name="store_fname" invisible="1" />
                        <field name="fecha" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="efemerides_view_tree" model="ir.ui.view">
        <field name="name">efemerides.view.tree</field>
        <field name="model">efemerides.efemerides</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="name" />
                <field name="contenido" />
                <field name="imagen" filename="store_fname"/>
                <field name="store_fname" invisible="1" />
                <field name="fecha" />
            </tree>
        </field>
    </record>

</odoo>