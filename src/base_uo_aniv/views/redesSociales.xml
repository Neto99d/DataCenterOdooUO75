<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="redesSociales_view_form" model="ir.ui.view">
        <field name="name">redesSociales.view.form</field>
        <field name="model">redesociales.redesociales</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name" readonly="1"/>
                        <field name="link_Facebook" />
                        <field name="link_Telegram" />
                        <field name="link_Twitter" />
                        <field name="link_Youtube" />
                        <field name="link_Linkedin" />
                        <field name="link_Instagram" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="redesSociales_view_tree" model="ir.ui.view">
        <field name="name">redesSociales.view.tree</field>
        <field name="model">redesociales.redesociales</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="name" />
                <field name="link_Facebook" />
                <field name="link_Telegram" />
                <field name="link_Twitter" />
                <field name="link_Youtube" />
                <field name="link_Linkedin" />
                <field name="link_Instagram" />
            </tree>
        </field>
    </record>

</odoo>