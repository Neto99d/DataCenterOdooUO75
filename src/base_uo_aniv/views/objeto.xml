<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="objeto_view_form" model="ir.ui.view">
        <field name="name">objeto.view.form</field>
        <field name="model">objeto.objeto</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name" readonly="1"/>
                        <field name="contenido" />
                        
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="objeto_view_tree" model="ir.ui.view">
        <field name="name">objeto.view.tree</field>
        <field name="model">objeto.objeto</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="name" />
                <field name="contenido" />
                
            </tree>
        </field>
    </record>

</odoo>
